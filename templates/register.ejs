  <script>
  var ham = document.querySelector(".navbar-toggler")
  ham.addEventListener("click", toggle)
  function toggle(){
    console.log(document.getElementById('navbarNavDropdown').classList.contains("show"))
    if (document.getElementById('navbarNavDropdown').classList.contains("show")){
      $( "#navbarNavDropdown" ).removeClass( "show" ).addClass( "hidden" )
    }
    else{
      $( "#navbarNavDropdown" ).removeClass( "hidden" ).addClass( "show" )
    }
  }
  </script>
  <script>
    $(document).ready(function() {
      $("#form").submit(function(event)
      {
        event.preventDefault();
        var formData = {
          player1ign : $("#player1ign").val(),
          player1chr : $("#player1chr").val(),
          player1discord : $("#player1discord").val(),
          player2ign : $("#player2ign").val(),
          player2chr : $("#player2chr").val(),
          player2discord : $("#player2discord").val(),
          player3ign : $("#player3ign").val(),
          player3chr : $("#player3chr").val(),
          player3discord : $("#player3discord").val(),
          player4ign : $("#player4ign").val(),
          player4chr : $("#player4chr").val(),
          player4discord : $("#player4discord").val(),
          phone :  $("#iglphone").val(),
          email :  $("#iglemail").val(),
          teamname : $("#teamname").val(),
          subs : $("#substitute").val(),
          typeCustom : $("#typeCustom").val()
        }
        checkEmail($("#iglemail").val(),formData);
      });
  });
  
  const checkEmail = (input,formData) => {
    var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    if(re.test(input.trim())){
        
      $.ajax({
        url: '/registerteam',
        type: 'POST',
        enctype: 'multipart/form-data',
        data: JSON.stringify(formData),
        contentType: false,
        dataType : 'text',
        cache: false,
        processData:false,
        success: function(response) 
        {
          document.getElementById('messageRes').innerHTML =  `<div class="res"><div role="alert">
            <div class="bg-green-500 text-white font-bold rounded-t px-4 py-2">Success</div>
            <div class="border border-t-0 border-green-400 rounded-b bg-green-100 px-4 py-3 text-green-700">
              <p>${response}</p>
            </div></div>`   
            document.getElementById('player1ign').value = ''
            document.getElementById('player1chr').value = ''
            document.getElementById('player1discord').value = ''
            document.getElementById('player2ign').value = ''
            document.getElementById('player2chr').value = ''
            document.getElementById('player2discord').value = ''
            document.getElementById('player3ign').value = ''
            document.getElementById('player3chr').value = ''
            document.getElementById('player3discord').value = ''
            document.getElementById('player4ign').value = ''
            document.getElementById('player4chr').value = ''
            document.getElementById('player4discord').value = ''
            document.getElementById('iglphone').value = ''
            document.getElementById('iglemail').value = ''
            document.getElementById('teamname').value = ''
            document.getElementById('substitute').value = ''
            setTimeout(function(){ $( ".res" ).remove(); }, 3000);
        }                    
      }); 
    }
    else{
      document.getElementById('messageRes').innerHTML =  `<div class="res"><div role="alert">
        <div class="bg-red-500 text-white font-bold rounded-t px-4 py-2">Invalid Email</div>
        <div class="border border-t-0 border-red-400 rounded-b bg-red-100 px-4 py-3 text-red-700">
          <p>Please enter a valid email id.</p>
        </div></div>` 
      setTimeout(function(){ $( ".res" ).remove(); }, 3000);
      return;
    }
  }
  
  </script>
