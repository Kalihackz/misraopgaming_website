    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.bundle.min.js" integrity="sha384-JEW9xMcG8R+pH31jmWH6WWP0WintQrMb4s7ZOdauHnUtxwoG2vI5DkLtS3qm9Ekf" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.1/dist/umd/popper.min.js" integrity="sha384-SR1sx49pcuLnqZUnnPwx6FCym0wLsk5JZuNx2bPPENzswTNFaQU1RDvt3wT4gWFG" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/js/bootstrap.min.js" integrity="sha384-j0CNLUeiqtyaRmlzUHCPZ+Gy5fQu0dQ6eZ/xAww941Ai1SxSY+0EQqNXNE6DZiVc" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script>
    var ham = document.querySelector(".navbar-toggler")
    ham.addEventListener("click", toggle)
    function toggle(){
      console.log(document.getElementById('navbarNavDropdown').classList.contains("show"))
      if (document.getElementById('navbarNavDropdown').classList.contains("show")){
        $( "#navbarNavDropdown" ).removeClass( "show" ).addClass( "hidden" )
      }
      else{
        $( "#navbarNavDropdown" ).removeClass( "hidden" ).addClass( "show" )
      }
    }

    function update(){
      fetch('/getteam/<%= room %> ')
      .then(response => response.json())
      .then(data => teamList(data));
    }

    function teamList(docs) {
      var str=''
      for(var i=0; i < docs.length; i++) {
         str = str + `<tr><th scope="row">${docs[i].slot}</th>
<td>${docs[i].teamname}</td>
<td><table class="table table-dark table-hover">
<tr><td>${docs[i].player1ign}</td></tr>
<tr><td>${docs[i].player2ign}</td></tr>
<tr><td>${docs[i].player3ign}</td></tr>
<tr><td>${docs[i].player4ign}</td></tr>
</table></td>
<td>${docs[i].player1ign}</td></tr>`
      }
       document.getElementById('tablebody').innerHTML=str;
  }
    update()

    setInterval( update, 20000); 
    </script>
  </body>
